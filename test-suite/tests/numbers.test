(with-test-prefix "+"

  (expect-fail "documented?"
    (documented? +))

  (with-test-prefix "wrong type argument"

    (pass-if-exception "1st argument string"
      exception:wrong-type-arg
      (+ "1" 2))

    (pass-if-exception "2nd argument bool"
      exception:wrong-type-arg
      (+ 1 #f))))
