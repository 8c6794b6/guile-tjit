(define-module (benchmarks srfi-1)
  #:use-module (srfi srfi-1)
  #:use-module (benchmark-suite lib))

(define %big-list
  (iota 1000000))

(define %small-list
  (iota 10))


(with-benchmark-prefix "fold"

  (benchmark "big" 30
    (fold (lambda (x y) y) #f %big-list))

  (benchmark "small" 2000000
    (fold (lambda (x y) y) #f %small-list)))


(with-benchmark-prefix "drop-while"

  (benchmark "big" 30
    (drop-while (lambda (n) #t) %big-list))

  (benchmark "small" 2000000
    (drop-while (lambda (n) #t) %small-list)))

(with-benchmark-prefix "map"

  (benchmark "big" 30
    (map (lambda (x) x) %big-list))

  (benchmark "small" 2000000
    (map (lambda (x) x) %small-list)))

(with-benchmark-prefix "for-each"

  (benchmark "big" 30
    (for-each (lambda (x) #f) %big-list))

  (benchmark "small" 2000000
    (for-each (lambda (x) #f) %small-list)))

